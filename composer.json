{
  "name": "portavice/permutation",
  "description": "Generating permutations of arrays normal and recursive",
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "homepage": "https://portavice.de",
  "keywords": [
    "permutation",
    "array",
    "recursive"
  ],
  "authors": [
    {
      "name": "Shaun LÃ¼deke",
      "email": "shaun.luedeke@portavice.de",
      "homepage": "https://portavice.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/portavice/Permutation/issues"
  },
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "phpcompatibility/php-compatibility": "^9.3.5",
    "phpunit/phpunit": "^10.2.2",
    "squizlabs/php_codesniffer": "^3.7.2"
  },
  "autoload": {
    "psr-4": {
      "Portavice\\Permutation\\": "src"
    },
    "psr-0": {
      "Portavice\\Permutation\\": "src"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs": ["phpcs"],
    "csfix": ["phpcbf"]
  },
  "autoload-dev": {
    "psr-4": {
      "Portavice\\Permutation\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
